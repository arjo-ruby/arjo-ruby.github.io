mail_msg
create table mail_msg( msg_id int not null auto_increment primary key, from_id integer not null, utc integer not null, thread_id integer not null, father integer not null, text varchar(1000) not null, status integer, foreign key(from_id) references user(id), foreign key(thread_id) references threads(thread_id), foreign key(status) references mail_statuses(s_id));


+-----------+---------------+------+-----+---------+----------------+
| Field     | Type          | Null | Key | Default | Extra          |
+-----------+---------------+------+-----+---------+----------------+
| msg_id    | int(11)       | NO   | PRI | NULL    | auto_increment |
| from_id   | int(11)       | NO   | MUL | NULL    |                |
| utc       | int(11)       | NO   |     | NULL    |                |
| thread_id | int(11)       | NO   | MUL | NULL    |                |
| father    | int(11)       | NO   |     | NULL    |                |
| text      | varchar(1000) | NO   |     | NULL    |                |
| status    | int(11)       | YES  | MUL | NULL    |                |
+-----------+---------------+------+-----+---------+----------------+



recipients
create table recipients (rec_id integer primary key auto_increment, msg_id integer, to_id integer, status integer, foreign key(msg_id) references mail_msg(msg_id), foreign key(to_id) references user(id), foreign key(status) references mail_statuses(s_id));

offset denotes seen or unseen.

+--------+---------+------+-----+---------+----------------+
| Field  | Type    | Null | Key | Default | Extra          |
+--------+---------+------+-----+---------+----------------+
| rec_id | int(11) | NO   | PRI | NULL    | auto_increment |
| msg_id | int(11) | YES  | MUL | NULL    |                |
| to_id  | int(11) | YES  | MUL | NULL    |                |
| status | int(11) | YES  |     | NULL    |                |
| offset | int(1)  | YES  |     | NULL    |                |
+--------+---------+------+-----+---------+----------------+



question

create table question(ques_id int(11) not null auto_increment comment 'Distinct id of questions', cat_id integer, class integer(2), ques_txt varchar(1000) not null comment 'text for the question', option1 varchar(255) not null,option2 varchar(255) not null, option3 varchar(255) not null,option4 varchar(255) not null,answer char(1) not null comment 'Correct option', ques_level int(5) not null comment 'Level of the question for adaptive strategy',foreign key(cat_id) references category(cat_id), primary key(ques_id));

+------------+---------------+------+-----+---------+----------------+
| Field      | Type          | Null | Key | Default | Extra          |
+------------+---------------+------+-----+---------+----------------+
| ques_id    | int(11)       | NO   | PRI | NULL    | auto_increment |
| cat_id     | int(11)       | YES  | MUL | NULL    |                |
| class      | int(2)        | YES  |     | NULL    |                |
| ques_txt   | varchar(1000) | NO   |     | NULL    |                |
| option1    | varchar(255)  | NO   |     | NULL    |                |
| option2    | varchar(255)  | NO   |     | NULL    |                |
| option3    | varchar(255)  | NO   |     | NULL    |                |
| option4    | varchar(255)  | NO   |     | NULL    |                |
| answer     | char(1)       | NO   |     | NULL    |                |
| ques_level | int(5)        | NO   |     | NULL    |                |
+------------+---------------+------+-----+---------+----------------+

seen_question

create table seen_question(ques_id int(11) not null,user_id int(11) not null comment 'Users has already seen this question', primary key(ques_id,user_id),foreign key(user_id) references user(id), foreign key(ques_id) references question(ques_id)); 

+---------+---------+------+-----+---------+-------+
| Field   | Type    | Null | Key | Default | Extra |
+---------+---------+------+-----+---------+-------+
| ques_id | int(11) | NO   | PRI | NULL    |       |
| user_id | int(11) | NO   | PRI | NULL    |       |
+---------+---------+------+-----+---------+-------+


Links

create table links(link_id integer primary key auto_increment, cat_id integer not null, class integer not null, section varchar(1) not null ,url varchar(200) not null, description varchar(100) not null, offset smallint not null, foreign key(cat_id) references category(cat_id))                   

+-------------+--------------+------+-----+---------+----------------+
| Field       | Type         | Null | Key | Default | Extra          |
+-------------+--------------+------+-----+---------+----------------+
| link_id     | int(11)      | NO   | PRI | NULL    | auto_increment |
| cat_id      | int(11)      | NO   | MUL | NULL    |                |
| class       | int(11)      | NO   |     | NULL    |                |
| section     | varchar(1)   | NO   |     | NULL    |                |
| url         | varchar(200) | NO   |     | NULL    |                |
| description | varchar(100) | NO   |     | NULL    |                |
| offset      | smallint(6)  | NO   |     | NULL    |                |
+-------------+--------------+------+-----+---------+----------------+


